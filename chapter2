      "source": [
        "Example: Multiple Conditions with Selected Columns\n",
        "\n",
        "Filter rows where Adj Close > 150 and Volume > 1,000,000, and keep only\n",
        "Adj Close and Daily % Change:"
      ],
      "metadata": {
        "id": "nv1tE5gNz-TP"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Filter rows and keep specific columns\n",
        "filtered_df = df_spy[(df_spy['Adj Close'] > 150) & (df_spy['Volume'] > 1_000_000)][['Adj Close', 'Daily % Change']]\n",
        "print(filtered_df)\n"
      ],
