            "2005-01-07  80.344254   80.459579\n",
            "2005-01-10  80.724121   80.344254\n",
            "2005-01-11  80.167862   80.724121\n",
            "2005-01-12  80.432434   80.167862\n",
            "2005-01-13  79.788002   80.432434\n",
            "2005-01-14  80.208595   79.788002\n"
          ]
        }
      ]
